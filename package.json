    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.12.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^36.0.1",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8"
